<% content_for :page_title do %>
  New Lookbook
<% end %>

<% content_for :page_actions do %>
  <%= link_to_with_icon 'arrow-left', 'Back To Lookbook List', admin_lookbooks_path, class: 'btn btn-default' %>
<% end %>

<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @lookbook} %>

<%= form_for [:admin, @lookbook] do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
