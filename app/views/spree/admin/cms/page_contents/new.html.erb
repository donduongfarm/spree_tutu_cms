<% content_for :page_title do %>
  <%= @page_content.section.name || 'Page Content' %>
<% end %>

<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @page_content} %>

<%= form_for [:admin, :cms, @page, @page_content] do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
